{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "MobiPurse",
  "appearance": {
    "default": "light",
    "strict": true
  },
  "colors": {
    "primary": "#16A34A"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Introduction",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "guides/introductions/getting-started",
              "guides/introductions/authorization",
              "guides/introductions/environments"
            ]
          },
          {
            "group": "Collect Payment",
            "pages": [
              "guides/payments/standard",
              "guides/payments/webhook"
            ]
          },
          {
            "group": "Disbursements",
            "pages": [
              "guides/disbursements/standard",
              "guides/disbursements/webhook"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "api-reference/openapi-3.1.yml"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Home",
          "href": "https://mobipurse.com",
          "icon": "house"
        },
        {
          "anchor": "Documentation",
          "href": "https://docs.mobipurse.com",
          "icon": "book-open-cover"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/mobipurse-logo.png",
    "dark": "/logo/mobipurse-logo.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@mobipurse.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://mobipurse.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  }
}
